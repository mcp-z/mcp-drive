{
  "name": "@mcp-z/mcp-drive",
  "version": "1.0.2",
  "description": "MCP server for Google Drive integration with file operations, folder navigation, search capabilities, and sharing management",
  "keywords": [
    "drive",
    "google-drive",
    "google",
    "mcp",
    "ai",
    "mcp-server",
    "model context protocol",
    "oauth",
    "google api",
    "file management",
    "cloud storage",
    "file search",
    "folder navigation",
    "drive api",
    "file operations",
    "ai agent",
    "claude",
    "llm",
    "file sharing",
    "google workspace",
    "document management",
    "productivity",
    "automation",
    "integration",
    "typescript",
    "nodejs",
    "api client"
  ],
  "homepage": "https://github.com/mcp-z/mcp-drive#readme",
  "bugs": {
    "url": "https://github.com/mcp-z/mcp-drive/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mcp-z/mcp-drive.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff.info@gmail.com>",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "bin": "bin/server.js",
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsc -p . --noEmit && tsds build",
    "examples:test": "node scripts/examples-run.ts",
    "format": "tsds format",
    "prepublish:check": "ncp",
    "prepublishOnly": "tsds validate",
    "test": "npm run test:unit && npm run test:integration",
    "test:engines": "nvu engines tsds test:node --no-timeouts 'test/**/*.test.ts'",
    "test:integration": "tsds test:node --no-timeouts 'test/integration/**/*.test.ts'",
    "test:setup": "node scripts/setup-test.ts",
    "test:unit": "tsds test:node --no-timeouts 'test/unit/**/*.test.ts'",
    "version": "tsds version"
  },
  "dependencies": {
    "@mcp-z/oauth": "^1.0.1",
    "@mcp-z/oauth-google": "^1.0.3",
    "@mcp-z/server": "^1.0.2",
    "@modelcontextprotocol/sdk": "^1.25.1",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "googleapis": "^169.0.0",
    "keyv-registry": "^0.4.0",
    "module-root-sync": "^2.0.2",
    "pino": "^10.1.0",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@mcp-z/client": "^1.0.3",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.0.3",
    "dotenv": "^17.2.3",
    "get-port": "^7.1.0",
    "keyv": "^5.5.5",
    "node-version-use": "^2.4.7",
    "ts-dev-stack": "^1.22.1",
    "tsds-config": "^1.0.4",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20"
  },
  "mcpName": "io.github.mcp-z/mcp-drive",
  "tsds": {
    "source": "src/index.ts"
  }
}
